{
    "name": "channel-api",
    "description": "To run the golang lambda service locally",
    "version": "1.0.0",
    "scripts": {
        "start": "node ./node_modules/.bin/serverless offline start --useDocker",
        "watch": "nodemon --watch ./ -e go,js --exec make build"
    },
    "devDependencies": {
        "nodemon": "^2.0.12",
        "serverless": "^2.57.0",
        "serverless-dynamodb-local": "^0.2.40",
        "serverless-offline": "^8.1.0"
    }
}
